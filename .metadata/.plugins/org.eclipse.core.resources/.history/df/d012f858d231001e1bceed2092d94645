package main;

import test.Recently_Played_Store;

public class Main_Method {

	 public static void main(String[] args) {
	
		  Recently_Played_Store store = new Recently_Played_Store(3); // capacity = 3


	        store.addRecentlyPlayedSong("user1", "S1");
	        store.addRecentlyPlayedSong("user1", "S2");
	        store.addRecentlyPlayedSong("user1", "S3");
	        System.out.println(store.getRecentlyPlayedSongs("user1")); // Output: [S3, S2, S1]

	        store.addRecentlyPlayedSong("user1", "S4");
	        System.out.println(store.getRecentlyPlayedSongs("user1")); // Output: [S4, S3, S2]

	        store.addRecentlyPlayedSong("user1", "S2");
	        System.out.println(store.getRecentlyPlayedSongs("user1")); // Output: [S2, S4, S3]

	        store.addRecentlyPlayedSong("user1", "S1");
	        System.out.println(store.getRecentlyPlayedSongs("user1")); // Output: [S1, S2, S4]
	    }
}